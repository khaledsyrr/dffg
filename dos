$(function() {
    var maxChars = 200; // الحد الأقصى لعدد الأحرف
    
    $("textarea.u-statc, textarea.u-topic, input.stopic, textarea.fl").on("input", function() {
        if ($(this).val().length > maxChars) {
            $(this).val($(this).val().substring(0, maxChars));
        }
    });
});











$.getScript('');

$(document).ready(function() {
    // إضافة مقياس البينغ المحسّن مع إشارة الواي فاي ووصف الحالة
    let pingWithWiFi = $(`
        <div id="ping-container" style="
            padding: 15px; 
            width: 98%; 
            margin: 10px auto 15px; 
            font-family: 'Tajawal', 'Aref Ruqaa', sans-serif; 
            border-radius: 15px; 
            background: linear-gradient(145deg, #ffffff, #f5f7fa);
            box-shadow: 0 10px 20px rgba(0, 0, 0, 0.08), 0 3px 6px rgba(0, 0, 0, 0.05);
            border: 1px solid rgba(230, 230, 230, 0.7);
            direction: rtl;
            transition: all 0.3s ease;
            position: relative;
            overflow: hidden;
        ">
            <div style="
                position: absolute;
                top: 0;
                right: 0;
                width: 80px;
                height: 80px;
                background: radial-gradient(circle at top right, rgba(66, 153, 225, 0.1), transparent 70%);
                border-radius: 0 0 0 100%;
                z-index: 0;
            "></div>
            
            <div id="ping-header" style="
                display: flex; 
                justify-content: space-between; 
                align-items: center;
                margin-bottom: 12px;
                position: relative;
                z-index: 1;
            ">
                <div style="display: flex; align-items: center;">
                    <span id="wifi-icon" class="fa fa-wifi" style="
                        font-size: 24px; 
                        color: #4299e1; 
                        margin-left: 10px;
                        filter: drop-shadow(0 2px 3px rgba(66, 153, 225, 0.3));
                        transition: all 0.5s ease;
                    "></span>
                    <span style="
                        font-weight: 700; 
                        font-size: 18px; 
                        color: #2d3748;
                    ">سرعة الاتصال</span>
                </div>
                <div id="ping-value-container" style="
                    background: rgba(237, 242, 247, 0.8); 
                    padding: 6px 14px; 
                    border-radius: 12px; 
                    display: flex; 
                    align-items: center;
                    box-shadow: inset 0 2px 4px rgba(0, 0, 0, 0.06);
                ">
                    <span id="ping" style="
                        font-weight: bold; 
                        font-size: 20px; 
                        color: #4299e1;
                        transition: color 0.5s ease;
                    ">--</span>
                    <span style="
                        font-size: 14px; 
                        margin-right: 4px; 
                        color: #718096;
                    ">ms</span>
                </div>
            </div>
            
            <div id="ping-meter-container" style="
                position: relative;
                margin: 15px 0;
                z-index: 1;
            ">
                <div id="ping-meter" style="
                    width: 100%; 
                    height: 12px; 
                    background-color: rgba(237, 242, 247, 0.8); 
                    border-radius: 10px; 
                    overflow: hidden;
                    box-shadow: inset 0 2px 4px rgba(0, 0, 0, 0.06);
                ">
                    <div id="ping-bar" style="
                        height: 100%; 
                        width: 0%; 
                        background: linear-gradient(90deg, #4299e1, #63b3ed); 
                        border-radius: 10px;
                        transition: width 0.8s cubic-bezier(0.34, 1.56, 0.64, 1), background 0.5s ease;
                        box-shadow: 0 0 10px rgba(66, 153, 225, 0.5);
                    "></div>
                </div>
                
                <div style="
                    display: flex;
                    justify-content: space-between;
                    margin-top: 6px;
                    font-size: 12px;
                    color: #718096;
                ">
                    <span>ممتاز</span>
                    <span>متوسط</span>
                    <span>ضعيف</span>
                </div>
            </div>
            
            <div id="ping-status-container" style="
                display: flex;
                align-items: center;
                background: rgba(237, 242, 247, 0.5);
                padding: 10px 15px;
                border-radius: 10px;
                margin-top: 10px;
                position: relative;
                z-index: 1;
            ">
                <div id="status-indicator" style="
                    width: 12px;
                    height: 12px;
                    border-radius: 50%;
                    background-color: #4299e1;
                    margin-left: 10px;
                    box-shadow: 0 0 8px rgba(66, 153, 225, 0.6);
                    transition: all 0.5s ease;
                "></div>
                <div id="ping-status" style="
                    font-weight: 600; 
                    font-size: 16px; 
                    color: #4299e1;
                    transition: color 0.5s ease;
                ">جاري فحص حالة الشبكة...</div>
            </div>
        </div>
    `);

    // إضافة العنصر قبل زر "حـفـظ"
    $("button:contains('حـفـظ')").before(pingWithWiFi);

    // تحميل خط Tajawal من Google Fonts إذا لم يكن موجودًا
    if (!$('link[href*="Tajawal"]').length) {
        $('head').append('<link href="https://fonts.googleapis.com/css2?family=Tajawal:wght@400;500;700&display=swap" rel="stylesheet">');
    }

    // إضافة تأثير تحميل أولي
    setTimeout(function() {
        updatePingMeter(Math.floor(Math.random() * 300) + 50);
    }, 1000);

    // تحديث قيمة البينغ بشكل تفاعلي كل 5 ثوانٍ مع تأثيرات انتقالية سلسة
    setInterval(function() {
        var pingValue = Math.floor(Math.random() * 300) + 50; // توليد قيمة بينغ عشوائية (من 50ms إلى 350ms)
        updatePingMeter(pingValue);
    }, 5000); // تحديث كل 5 ثوانٍ

    // دالة لتحديث مقياس البينغ بشكل متكامل
    function updatePingMeter(pingValue) {
        var pingText = pingValue + "";
        var pingPercentage = Math.min((pingValue / 350) * 100, 100); // حساب نسبة البينغ بناءً على القيمة

        // تغيير النص مع تأثير عداد
        animateCounter($("#ping"), pingText);

        // تحديث شريط المقياس بتأثير انتقالي سلس
        $("#ping-bar").css("width", pingPercentage + "%");

        // تغيير الألوان والحالة بناءً على قيمة البينغ
        if (pingValue < 100) {
            // سريع - أخضر
            updateColors("#48bb78", "ممتازة", "linear-gradient(90deg, #48bb78, #68d391)");
        } else if (pingValue >= 100 && pingValue <= 200) {
            // متوسط - أزرق
            updateColors("#4299e1", "جيدة", "linear-gradient(90deg, #4299e1, #63b3ed)");
        } else if (pingValue > 200 && pingValue <= 250) {
            // بطيء نوعًا ما - برتقالي
            updateColors("#ed8936", "متوسطة", "linear-gradient(90deg, #ed8936, #f6ad55)");
        } else {
            // بطيء جدًا - أحمر
            updateColors("#e53e3e", "ضعيفة", "linear-gradient(90deg, #e53e3e, #fc8181)");
        }
    }

    // دالة لتحديث الألوان والحالة
    function updateColors(color, status, gradient) {
        // تحديث لون النص والأيقونة
        $("#ping").css("color", color);
        $("#wifi-icon").css("color", color);
        $("#ping-status").text("حالة الشبكة: " + status).css("color", color);
        $("#status-indicator").css({
            "background-color": color,
            "box-shadow": "0 0 8px " + color + "aa"
        });
        
        // تحديث لون شريط المقياس
        $("#ping-bar").css("background", gradient);
    }

    // دالة لإنشاء تأثير عداد للأرقام
    function animateCounter(element, newValue) {
        var currentValue = parseInt(element.text()) || 0;
        var duration = 500; // مدة التأثير بالمللي ثانية
        var startTime = null;
        
        function animate(timestamp) {
            if (!startTime) startTime = timestamp;
            var progress = timestamp - startTime;
            var percentage = Math.min(progress / duration, 1);
            
            var value = Math.floor(currentValue + (newValue - currentValue) * percentage);
            element.text(value);
            
            if (percentage < 1) {
                window.requestAnimationFrame(animate);
            }
        }
        
        window.requestAnimationFrame(animate);
    }
});

